{
  "version": 2,
  "name": "ultimate-pokedex",
  "alias": "ultimate-pokedex",
  "build": {
    "env": {
      "ALGOLIA_INDEX_NAME": "@pokedex_algolia_index_name",
      "ALGOLIA_APP_ID": "@pokedex_algolia_app_id",
      "ALGOLIA_API_KEY": "@pokedex_algolia_api_key"
    }
  },
  "env": {
    "DATABASE_HOST": "@pokedex_db_host",
    "DATABASE_USER": "@pokedex_db_user",
    "DATABASE_PASSWORD": "@pokedex_db_password",
    "DATABASE_NAME": "@pokedex_db_database"
  }
}
