      "version": {
        "name": "ultra-sun",
        "url": "https://pokeapi.co/api/v2/version/29/"
      }
    },
    {
      "flavor_text": "Ha sempre un sacco di idee che potrebbero\ncambiare il mondo, ma le dimentica un attimo\ndopo che gli sono venute in mente.",
      "language": {
        "name": "it",
        "url": "https://pokeapi.co/api/v2/language/8/"
