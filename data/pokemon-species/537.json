      }
    },
    {
      "flavor_text": "They shoot paralyzing liquid from their head bumps.\nThey use vibration to hurt their opponents.",
      "language": {
        "name": "en",
        "url": "https://pokeapi.co/api/v2/language/9/"
      },
      "version": {
        "name": "alpha-sapphire",
