      }
    },
    {
      "flavor_text": "Ricava energia dal sole e dall’acqua grazie alle foglie che ha\nsul dorso. In questo modo può sopravvivere anche senza\nmangiare.",
      "language": {
        "name": "it",
        "url": "https://pokeapi.co/api/v2/language/8/"
      },
      "version": {
        "name": "alpha-sapphire",
        "url": "https://pokeapi.co/api/v2/version/26/"
