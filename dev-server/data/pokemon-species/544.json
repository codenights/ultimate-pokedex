      }
    },
    {
      "flavor_text": "It is usually motionless, but when attacked,\nit rotates at high speed and then crashes into\nits opponent.",
      "language": {
        "name": "en",
        "url": "https://pokeapi.co/api/v2/language/9/"
      },
      "version": {
        "name": "alpha-sapphire",
