      "version": {
        "name": "alpha-sapphire",
        "url": "https://pokeapi.co/api/v2/version/26/"
      }
    },
    {
      "flavor_text": "El cuerpo de Cinccino está recubierto por una grasa especial\nque hace que los golpes y ataques enemigos no acierten.",
      "language": {
        "name": "es",
        "url": "https://pokeapi.co/api/v2/language/7/"
      },
      "version": {
        "name": "alpha-sapphire",
        "url": "https://pokeapi.co/api/v2/version/26/"
      }
    },
    {
      "flavor_text": "Sein Körper ist mit einem speziellen Öl überzogen, an dem\ngegnerische Hiebe und ähnliche Angriffe einfach abgleiten.",
      "language": {
        "name": "de",
        "url": "https://pokeapi.co/api/v2/language/6/"
