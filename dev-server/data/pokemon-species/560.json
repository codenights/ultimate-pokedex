  "flavor_text_entries": [
    {
      "flavor_text": "性格粗鲁，但是对于自己的家族、\n群体的伙伴和领地，非常重视。",
      "language": {
        "name": "zh-Hans",
        "url": "https://pokeapi.co/api/v2/language/12/"
      },
      "version": {
        "name": "ultra-sun",
